{
    "name": "mrbohem/screen-wire",
    "description": "https://github.com/mrbohem/screen-wire.git",
    "keywords": [
        "mrbohem",
        "laravel",
        "screen-wire"
    ],
    "homepage": "https://github.com/mrbohem/screen-wire",
    "license": "MIT",
    "authors": [
        {
            "name": "Shivam",
            "email": "09sh27up@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37",
        "livewire/livewire": "^2.6",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Mrbohem\\ScreenWire\\": "src",
            "Mrbohem\\ScreenWire\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrbohem\\ScreenWire\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrbohem\\ScreenWire\\ScreenWireServiceProvider"
            ],
            "aliases": {
                "ScreenWire": "Mrbohem\\ScreenWire\\ScreenWireFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
